@using  CarManagement.Core.Models.DTOs
@model IEnumerable<WheelDto>

<h3>Wheels</h3>
<table>
    <thead>
        <tr>
            <th>Door No.</th>
            <th>State</th>
        </tr>
    </thead>
    <tbody>
        @{
            WheelDto[] wheels = Model.ToArray();
        }
        @for (int i = 0, n = wheels.Length; i < n; i++)
        {
            <tr>
                <td>@i</td>
                <td>@wheels[i].Pressure</td>
            </tr>
        }
    </tbody>
</table>